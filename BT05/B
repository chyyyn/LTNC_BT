1. bool majority(bool a, bool b, bool c){
    return a || b || c ? (a && b) || (a && c) || (b && c) : false;
    }
2. bool eq(int*a , int*b, int n){
        int cnt=0;
        for (int i=0; i<size; i++){
            if (a[i]==b[i]) cnt++;
        }
        if (cnt==n) return true;
    }
